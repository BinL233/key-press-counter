{
  "name": "key-press-counter",
  "displayName": "KeyPressCounter",
  "description": "Tools for monitoring keyboard usage in VSCode.",
  "version": "0.0.1",
  "publisher": "BinL",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/BinL233/key-press-counter"
  },
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "key-press-counter.toggle",
        "title": "KeyPressCounter: Active/Inactive"
      },
      {
        "command": "key-press-counter.setInfoShowNum",
        "title": "KeyPressCounter: Keyboard count Message"
      },
      {
        "command": "key-press-counter.resetCounter",
        "title": "KeyPressCounter: Reset Counter"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
